<launch>
  <!-- Arguments -->

  <arg name="map_file" default="/home/intemslab/map.yaml"/>
  <arg name="move_forward_only" default="true"/>
  <arg name="param_folder" default="$(find cube_robot_navigation)/param/pre-hydro"/>

  <!-- Map server -->
  <node pkg="map_server" name="map_server" type="map_server" args="$(arg map_file)"/>

  <!-- AMCL -->
  <include file="$(find cube_robot_navigation)/launch/amcl.launch"/>

  <!-- move_base -->
   <include file="$(find cube_robot_navigation)/launch/move_base.launch">
        <arg name="move_forward_only" value="$(arg move_forward_only)"/>
        <arg name="param_folder" value="$(arg param_folder)"/>
  </include> 
</launch>
