<launch>
  <node pkg="cube_robot" type="tf_pub.py" name="left_wheel"  output="screen">
  	<param name="x_coord" value="1.0" />
        <param name="y_coord" value="2.0" />
        <param name="t_coord" value="0.0" />
        <param name="tf_to" value="left_wheel" />
 </node>
<node pkg="cube_robot" type="tf_pub.py" name="right_wheel"  output="screen">
  	<param name="x_coord" value="1.0" />
        <param name="y_coord" value="2.0" />
        <param name="t_coord" value="0.0" />
        <param name="tf_to" value="right_wheel" />
 </node>
 <node pkg="cube_robot" type="tf_pub.py" name="camera"  output="screen">
  	<param name="x_coord" value="1.0" />
        <param name="y_coord" value="2.0" />
        <param name="t_coord" value="0.0" />
        <param name="tf_to" value="camera" />
 </node>
 <node pkg="cube_robot" type="tf_pub.py" name="lidar"  output="screen">
  	<param name="x_coord" value="1.0" />
        <param name="y_coord" value="2.0" />
        <param name="t_coord" value="0.0" />
        <param name="tf_to" value="lidar" />
 </node>
</launch>
